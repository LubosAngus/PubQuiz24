{
  "name": "pub-quiz-24",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrations:create": "knex migrate:make",
    "db:migrations:migrate": "knex migrate:latest && npm run db:export-types",
    "db:migrations:rollback": "knex migrate:rollback && npm run db:export-types",
    "db:export-types": "npx @rmp135/sql-ts -c ./database/types/export-types-config.json"
  },
  "dependencies": {
    "@primevue/themes": "^4.2.0",
    "better-sqlite3": "^9.6.0",
    "knex": "^3.1.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.13.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.6.1",
    "@primevue/nuxt-module": "^4.2.0",
    "@rmp135/sql-ts": "^2.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3",
    "sass-embedded": "^1.80.4",
    "typescript": "^5.6.3"
  }
}